{
  "name": "PWA",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": " node --inspect server/server.js",
    "build": "rimraf build && next build &&  workbox injectManifest workbox-config.js && node customminify --rootpath build --mode css ",
    "build:sit": "rimraf build && next build && node-gyp configure build && workbox injectManifest workbox-config-sit.js  && node customminify --rootpath build --mode css",
    "build:qa": "rimraf build && next build && node-gyp configure build && workbox injectManifest workbox-config-qa.js  && node customminify --rootpath build --mode css",
    "build:prod": "rimraf build && next build && node-gyp configure build && workbox injectManifest workbox-config.js  && node customminify --rootpath build --mode css",
    "build:w": " next build && node-gyp configure build && workbox injectManifest workbox-config.js && node customminify --rootpath build --mode css  && pm2 startOrRestart ecosystem.config.js --env local",
    "start": "pm2 startOrRestart ecosystem.config.js --env dev",
    "export": "npm run build && next export",
    "pm2:local": "pm2 startOrRestart ecosystem.config.js --env local",
    "pm2:sit": "pm2 startOrRestart ecosystem.config.js --env sit",
    "pm2:qa": "pm2 startOrRestart ecosystem.config.js --env qa",
    "pm2:stop": "pm2 stop all",
    "pm2:delete": "pm2 delete all",
    "pm2:restart": "pm2 restart all",
    "pm2:logs": "pm2 logs",
    "pm2:list": "pm2 list"
  },
  "keywords": [
    "react",
    "hooks",
    "nextjs"
  ],
  "author": "Piyush",
  "license": "ISC",
  "devDependencies": {
    "file-loader": "^2.0.0",
    "mini-css-extract-plugin": "^0.5.0",
